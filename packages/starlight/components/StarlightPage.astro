---
import { attachRouteDataAndRunMiddleware } from '../utils/routing/middleware.js';
import {
	generateStarlightPageRouteData,
} from '../utils/starlight-page.js';
import Page from './Page.astro';

await attachRouteDataAndRunMiddleware(
	Astro,
	await generateStarlightPageRouteData({ props: Astro.props, context: Astro })
);
---

<Page><slot /></Page>
